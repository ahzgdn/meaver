<div id="main">

  <h1>Gallery</h1>
  <div id="posts" class="transitions-enabled">
  

      <% @posts.each do |post| %>
      <div class="box">
        
          <%= link_to image_tag(post.image.url(:medium)), post %></br>
          <%= link_to 'Edit', edit_post_path(post) %></br>
          <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></br>
          </div>
      <% end %>
      

  <br>

  <%= link_to 'New Post', new_post_path %>
</div>
</div>
